programa {
  funcao inicio() {
    escreva("Qual seu nome:")
    cadeia nome
    leia(nome)
    escreva("Qual sua idade:")
    inteiro idade
    leia(idade)
    escreva("Qual sua altura:")
    real altura
    leia(altura)
    escreva("Nome: ", nome, "\n", "Idade: ", idade, "\n", "Altura: ", altura)
  }
}
